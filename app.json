{
  "expo": {
    "name": "brainhop",
    "slug": "brainhop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brainhop_logo_adaptiv_new.png",
    "scheme": "brainhop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "backgroundColor": "#ffffff",

    "splash": {
      "image": "./assets/images/brainhop_logo_big.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/brainhop_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cycrqr.brainhop"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/brainhop_logo_big.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f97202b-7c40-4693-bcb4-e3ab19b72a44"
      }
    }
  }
}
